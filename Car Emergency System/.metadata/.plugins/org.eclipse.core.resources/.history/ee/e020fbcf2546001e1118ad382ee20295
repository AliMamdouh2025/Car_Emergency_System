#ifndef INERNAL_EEPROM_INTERFACE_H_
#define INERNAL_EEPROM_INTERFACE_H_

#include "std_types.h"

#define BOOTLOADER_USED 1
#define BOOTLOADER_UNUSED 0
#define EEPROM_INTERRUPT_ENABLE 1
#define EEPROM_INTERRUPT_DISABLE 0


void Internal_EEPROM_init(void);
void EEPROM_write(unsigned int uiAddress, unsigned char ucData);
unsigned char EEPROM_read(unsigned int uiAddress);




#endif /* INERNAL_EEPROM_INTERFACE_H_ */
